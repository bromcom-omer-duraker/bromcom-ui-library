{
  "name": "bromcom-ui",
  "version": "1.7.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bromcom-ui/bromcom-ui.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "gulp svg && stencil build",
    "start": "gulp svg && stencil build --dev",
    "start:watch": "gulp svg && stencil build --dev --watch",
    "serve": "gulp svg && stencil build --dev --watch --serve",
    "generate": "gulp svg && stencil generate",
    "storybook": "start-storybook -p 8000 -c .storybook -s www",
    "story": "npm-run-all -p start:watch storybook",
    "build:storybook": "build-storybook -s www",
    "prebuild:storybook": "npm start",
    "prepublish": "npm run build"
  },
  "dependencies": {},
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "@stencil/core": "^2.0.1",
    "@stencil/sass": "^1.3.2",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-backgrounds": "^6.1.9",
    "@storybook/addon-controls": "^6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.1.9",
    "@storybook/html": "^6.0.28",
    "babel-loader": "^8.2.0",
    "classnames": "^2.2.6",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "jsx-dom": "^6.4.23",
    "npm-run-all": "^4.1.5"
  }
}
